capsule {
  id (example.spaceResorts)
  version (1.0.0)
  format (3)
  targets {
    target (bixby-mobile-en-US)
    target (bixby-mobile-ko-KR)
  }
  runtime-flags {
    concepts-inherit-super-type-features
    support-halt-effect-in-computed-inputs
    modern-prompt-rejection
    allow-dialogs-on-detail-pages
    modern-default-view-behavior
    no-fallback-to-result-collections
    no-filtering-with-validation
    use-input-views-for-selection-list-detail
  }
  // permissions {
  //   user-profile-access
  // }
  capsule-imports {
    import (viv.core) {
      as (core)
    }
    import (viv.self) {
      as (self)
      version (3.5.167)
    }
    import (viv.contact) {
      as (contact)
      version-from (self)
    }
    import (viv.entity) {
      as (entity)
      version-from (self)
    }
    import (viv.common) {
      as (common)
      version-from (entity)
    }
    import (viv.time) {
      as (time)
      version-from (entity)
    }
    import (viv.money) {
      as (money)
      version (2.22.20)
    }
  }
  store-sections{
    section (TravelAndTransportation)
  }
}
